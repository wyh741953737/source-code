{% from "macor/product.njk" import sourceProd %}

{% macro hydrate(opt, params) %}
<div v-if="!list.length" class="product-box" render="njk">
  {% for prod in opt.data %}
  {% if opt.source %}
  {{ sourceProd(prod, params) }}
  {% else %}
  {{ product(prod, params) }}
  {% endif %}
  {% endfor %}
</div>
<div v-else class="product-box" render="vue">
  {% if opt.source %}
  <product-source v-for="(prod, idx) of list" :prod="prod" :key="idx" :img-Size="imgSize" />
  {% else %}
  <product-card
    tracking='{"clickId": "{{ opt.tracking.clickId }}", "collectionId": "{{ opt.tracking.collectionId }}", "addToQueueId": "{{ opt.tracking.addToQueueId }}", "exposureId": "{{ opt.tracking.exposureId }}"}' v-for="(prod, idx) of list" :prod="prod" :key="idx" :img-Size="imgSize" title="{{ opt.title }}" />
  {% endif %}
</div>
{% endmacro %}

