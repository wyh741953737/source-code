                  <div class="select-item" v-for="f of flagList" @click="clickFlag(f)">
                    <img v-show="language == f" src="/egg/image/yes.png" alt="" class="yes-img" style="width: 15px; height: 10px;">
                    <img :src="flagSrc(f)" :alt="f.opt" style="border: 1px solid #e5e5e5;">
                    <span>${ f.name }</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="item bottom">
            <div class="title">{{ i18next.t('common-top-currency') }}</div>
            <div class="info-show" @click.stop="clickSelectBox2">
              <div class="text">${ currency.currency }</div>
              <span class="icon"></span>
              <div class="select-box" v-if="visibleSelect2" @click.stop>
                <!-- <div class="select-search"> UI统一，搜索先砍掉 20-04-28
                  <input type="text" placeholder="Search" />
                </div> -->
                <div class="select-list">
                  <div class="select-item" v-for="c of currencyList" @click="clickCurrency(c)">
                    <img v-show="currency.currency == c.currency" src="/egg/image/yes.png" alt="" class="yes-img" style="width: 15px; height: 10px;">
                    <span>${ c.currency }</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="button cursor-pointer rounded-sm new-text-theme" @click="clickConfirm">{{ i18next.t('common-top-confirm') }}</div>
        </div>
      </div>
    </div>
    <div id="vue-top-nav" v-cloak>
      <ul class="short-nav h-100 d-flex align-items-center text-center" @mouseover="warehousesMouseover()">
        {% for nav in topNavList %}
        <li class="nav-item" {% if nav.child %} @mouseover="topnavOver{{nav.id}}=true" @mouseleave="topnavOver{{nav.id}}=false" :class="{'topnav-hover': topnavOver{{nav.id}} == true}" {% endif %} id="navItem{{nav.id}}">
          {% if nav.id == 5 %}
          <a v-cjstat='{"pageCode":"home", "eventType": 1, "buttonName": "{{ nav.name }}","buttonType": 0}' @click="updateAuthHrefNew('{{ nav.href }}', '{{ nav.auth }}')" id="myCJOutLine" class="myCJOutLine"></a>
          {% endif %}
          <a v-cjstat='{"pageCode":"home", "eventType": 1, "buttonName": "{{ nav.name }}","buttonType": 0}' @click="updateAuthHref('{{ nav.href }}', '{{ nav.auth }}')" href="{{ nav.href }}" class="nav-name {{'active' if nav.active else ''}}">{{ nav.name }}
            {% if nav.child %}
            <i v-if="topnavOver{{nav.id}}" class="iconfont iconshang1 font-size-12 pl-5"></i>
            <i v-if="!topnavOver{{nav.id}}" class="iconfont iconxia2 font-size-12 pl-5"></i>
            {% endif %}
          </a>
          {% if nav.child %}
            <!-- <ul class="child-list" :class="{'Authorization-box': nav.name == 'Authorization', 'Warehouses-box': nav.name == 'Warehouses'}"> -->
              {% set childListClass = 'child-list' %}
              {% if nav.id == 1 %}
              {% set childListClass = 'child-list Authorization-box' %}
              {% endif %}
              {% if nav.id == 2 %}
              {% set childListClass = 'child-list Warehouses-box' %}
              {% endif %}
              <div class="child-list-box arrow-container" @mouseover="topnavOver{{nav.id}}=true">
                <ul class="{{ childListClass }}">
                  {% for ch in nav.child %}
                  <li class="ch-item item-h">
                    <a v-cjstat='{"pageCode":"home", "eventType": 1, "buttonName": "{{ch.name }}","buttonType": "1"}' @click="updateAuthHref('{{ ch.href }}', '{{ ch.auth }}')" href="{{ ch.href }}">{{ ch.name }}</a>
                  </li>
                  {% endfor %}
                </ul>
                {% if nav.id == 2 %}
                <div class="arrow" @click="warehousesArrowClick()">
                  <i class="iconfont iconxiangxiashuangjiantou"></i>
                </div>
                {% endif %}
              </div>
          {% endif %}
        </li>
        {% endfor %}
      </ul>
    </div>
