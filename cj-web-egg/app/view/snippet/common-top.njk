        <img :src="flagImg" width="24px" alt="Flag">
        /
        <span class="currency">${ currency.currency }</span>
        <i v-if="visibleLang" class="iconfont iconshang1 font-size-12 pl-5"></i>
        <i v-if="!visibleLang" class="iconfont iconxia2 font-size-12 pl-5"></i>
      </div>
      <div class="suspence bg-white" v-if="visibleLang" @click="clickSuspence" id='language-suspence'>
