{
  "name": "cj-web-egg",
  "version": "1.0.0",
  "main": "index.js",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "devs": "npm run before-dev && concurrently -c=blue,green,yellow -n=\"egg \",gulp,mycj -p=[{name}] \"npm run egg-dev\" \"npm run gulp\"",
    "dev": "npm run before-dev && concurrently -c=blue,green,yellow -n=\"egg \",gulp,mycj -p=[{name}] \"npm run egg-dev\" \"npm run gulp\" \"npm run cj-dev\" --hot",
    "test": "npm run before-test && npm run cj-build && npm run gulp",
    "testnew": "npm run before-testnew && npm run cj-build && npm run gulp",
    "cn": "npm run before-cn && npm run cj-build && npm run gulp",
    "build-cn": "npm run before-cn && npm run cj-build && npm run gulp",
    "build": "npm run before-start && npm run cj-build && npm run gulp",
    "start-frontend": "egg-scripts start --title=server-cjseo",
    "start": "npm run egg-start",
    "stop": "npm run egg-stop",
    "egg-dev": "egg-bin dev",
    "egg-start": "egg-scripts start --daemon --title=server-cjseo",
    "egg-stop": "egg-scripts stop --title=server-cjseo",
    "gulp": "node script/gulp.js",
    "before-dev": "node script/before.js --env=development",
    "before-test": "node script/before.js --env=test",
    "before-testnew": "node script/before.js --env=testnew",
    "before-cn": "node script/before.js --env=production-cn",
    "before-start": "node script/before.js --env=production",
    "-mycj-s-": "---------mycj脚本---------s",
    "cj-dev": "npm run cj-start",
    "cj-start": "node mycj/script/before.js --env=development && node mycj/script/build.js --watch",
    "cj-build": "node mycj/script/before.js && node mycj/script/build.js && node mycj/script/after.js",
    "-mycj-e-": "---------mycj脚本---------e",
    "i18n-push:test": "i18n-util --type push --env test",
    "i18n-push:production": "i18n-util --type push --env production",
    "i18n-pull:test": "i18n-util --type pull --env test",
    "i18n-pull:production": "i18n-util --type pull --env production",
    "i18n-generate": "i18n-util --type generateDevLang --inputPath public/lang/local --watch"
  },
  "dependencies": {
    "@cckj/js-tracking-report": "^1.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "cckj-i18n-util": "^0.2.9",
    "egg": "^2.26.0",
    "egg-alinode": "^2.0.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.13.0",
    "egg-view-nunjucks": "^2.2.0",
    "i18next": "^19.4.5",
    "js-base64": "^3.5.2",
    "js-cookie": "^2.2.1",
    "jsencrypt": "^3.0.0-rc.1",
    "jwt-decode": "^3.1.2",
    "lottie-web": "^5.7.0",
    "module-alias": "^2.2.2",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-loader": "^8.0.6",
    "babel-preset-latest": "^6.24.1",
    "chalk": "^3.0.0",
    "cj-lang-build": "^1.0.7",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "egg-bin": "^4.14.1",
    "egg-http-proxy": "^1.0.0",
    "file-loader": "^5.1.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-collector": "^1.3.3",
    "gulp-rollup-each": "^3.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimist": "^0.6.1",
    "ora": "^4.0.3",
    "rollup": "^2.0.2",
    "rollup-plugin-vue": "^5.1.7",
    "style-loader": "^1.1.3",
    "through2": "^3.0.1",
    "url-loader": "^3.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-merge": "^4.2.2",
    "webpack-stats-plugin": "^0.3.1"
  },
  "_moduleAliases": {
    "@root": "."
  }
}
