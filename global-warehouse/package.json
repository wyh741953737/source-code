{
  "name": "globalWarehouse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "doc": "node docs/config/index.js",
    "web": "cross-env platform=web GLOBAL_ENV=local umi dev",
    "web:proxy": "cross-env platform=web GLOBAL_ENV=local GLOBAL_SERVICE=proxy umi dev",
    "web:master": "cross-env platform=web GLOBAL_ENV=master umi dev",
    "web:release": "cross-env platform=web GLOBAL_ENV=release umi dev",
    "web:prod": "cross-env platform=web GLOBAL_ENV=prod umi dev",
    "build": "cross-env platform=web GLOBAL_ENV=prod umi build",
    "build:release": "cross-env platform=web GLOBAL_ENV=release umi build",
    "build:master": "cross-env platform=web GLOBAL_ENV=master umi build",
    "build:local": "cross-env platform=web GLOBAL_ENV=local umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "umi-test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "i18n-generate": "i18n-util --type generateDevLang --inputPath public/lang/local --watch",
    "reportWorlds:test": "i18n-util --type push --env test",
    "reportWorlds:master": "i18n-util --type push --env master",
    "reportWorlds:release": "i18n-util --type push --env release",
    "reportWorlds:prod": "i18n-util --type push --env prod",
    "pullWorlds:master": "i18n-util --type pull --env master",
    "pullWorlds:release": "i18n-util --type pull --env release",
    "pullWorlds:prod": "i18n-util --type pull --env prod",
    "scanner": "i18next-scanner"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/react-native": "^4.0.7",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-react": "^7.13.13",
    "@cckj/cj-utils": "^1.3.2",
    "@cckj/i18n-util": "^0.2.7",
    "@cckj/react-native-tracking": "^0.4.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cameraroll": "^4.0.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^5.0.11",
    "@react-navigation/bottom-tabs": "^5.11.9",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@umijs/preset-react": "1.x",
    "antd": "^4.15.2",
    "antd-mobile": "^2.3.4",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-root-import": "^6.6.0",
    "better-scroll": "^2.2.1",
    "classnames": "^2.3.1",
    "concurrently": "^6.0.1",
    "cross-env": "^7.0.3",
    "good-storage": "^1.1.1",
    "i18next": "^20.2.1",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-scanner": "^3.0.0",
    "install": "^0.13.0",
    "js-tracking-report": "^1.0.3",
    "lottie-web-react": "^1.0.3",
    "marked": "^2.0.3",
    "postcss-plugin-px2rem": "^0.8.1",
    "react": "17.0.1",
    "react-i18next": "^11.8.12",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-i18n": "^2.0.15",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.1.0",
    "react-native-refresh-list-view": "^1.0.12",
    "react-native-render-html": "^5.1.0",
    "react-native-root-siblings": "^3.2.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.0.0",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.3.2",
    "react-uuid": "^1.0.2",
    "reactjs-swiper": "^0.1.0",
    "rmc-notification": "^1.0.0",
    "swiper": "^6.6.2",
    "umi": "^3.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/good-storage": "^1.1.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.64.0",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@umijs/test": "^3.4.7",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-native-iconfont-cli": "^2.2.1",
    "react-test-renderer": "17.0.1",
    "tslib": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
